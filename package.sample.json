{
  "name": "halo",
  "description": "Openstack NodeJS portable server.",
  "version": "0.0.1",
  "contributors": [
    "gogocrow <gogocrow@163.com>",
    "majiajun <majiajun@unitedstack.com>",
    "Lee Yao <yaoli@unitedstack.com>"
  ],
  "license": "MIT",
  "repository": "http://git.ustack.com/ustack/halo",
  "keywords": [
    "OpenStack",
    "API"
  ],
  "main": "index.js",
  "scripts": {
    "build": "./scripts/halo_build.sh",
    "start": "./scripts/start.sh",
    "restart": "pm2 restart pm2.json",
    "test": "./scripts/test.sh",
    "eslint": "eslint *.js server/**/ configs/ scripts/ client/**/ tests/__tests__/ --ext .js,.jsx",
    "dev": "cd client && rm -rf dist/*.js dist/*.css dist/*.map && node ../scripts/client_i18n_build.js && webpack --config dev.webpack.config.js",
    "benchmark": "./scripts/benchmark.sh",
    "prepublish": "cd client/uskin && npm install",
    "transpile": "node ./scripts/client_i18n_build.js && node ./scripts/server_i18n_build.js",
    "assemble": "node ./scripts/assemble.js",
    "pull": "node ./scripts/pull.js && node ./scripts/merge_config.js",
    "merge": "node ./scripts/merge_config.js OVERWRITE",
    "tags": "node ./scripts/get_tag.js"
  },
  "dependencies": {
    "body-parser": "~1.14.1",
    "connect-memjs": "0.1.0",
    "connect-redis": "^3.0.1",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.0",
    "ejs": "^2.3.4",
    "errorhandler": "^1.4.2",
    "eventemitter2": "^1.0.0",
    "express": "~4.13.3",
    "express-session": "^1.12.1",
    "glob": "^6.0.1",
    "i18n-2": "^0.6.0",
    "memjs": "^0.10.0",
    "morgan": "~1.6.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "redis": "^2.4.2",
    "winston": "^2.1.1",
    "ws": "^1.0.1",
    "utf-8-validate": "^1.2.1",
    "bufferutil": "^1.2.1",
    "vsphere-connect": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.0.12",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.0.12",
    "babel-preset-react": "^6.0.12",
    "babel-runtime": "^6.3.19",
    "chalk": "^1.1.1",
    "clone": "^1.0.2",
    "css-loader": "^0.23.0",
    "cssnano": "^3.4.0",
    "deep-equal": "^1.0.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "grunt": "^0.4.5",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-webpack": "^1.0.11",
    "istanbul": "^0.4.1",
    "jasmine": "^2.4.1",
    "jest-cli": "^11.0.2",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "load-grunt-tasks": "^3.3.0",
    "postcss-loader": "^0.8.2",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "^15.0.1",
    "rsvp": "^3.1.0",
    "supertest": "^1.1.0",
    "time-grunt": "^1.2.2",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.14.0"
  },
  "engines": {
    "node": ">= 5.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest/index.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testDirectoryName": "tests/static_tests",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/client"
  }
}
