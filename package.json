{
  "name": "halo",
  "description": "Openstack NodeJS portable server.",
  "version": "0.0.1",
  "contributors": [
    "gogocrow <gogocrow@163.com>",
    "majiajun <majiajun@unitedstack.com>",
    "Lee Yao <yaoli@unitedstack.com>"
  ],
  "license": "MIT",
  "repository": "http://git.ustack.com/ustack/halo",
  "keywords": [
    "OpenStack",
    "API"
  ],
  "main": "index.js",
  "scripts": {
    "build": "cd static && grunt build",
    "start": "./start.sh",
    "test": "jest",
    "eslint": "eslint *.js server/ static/**/ tests/__tests__/ --ext .js,.jsx",
    "devjs": "cd static && webpack --config dev.webpack.config.js"
  },
  "dependencies": {
    "async": "^1.5.0",
    "body-parser": "~1.14.1",
    "connect-redis": "^3.0.1",
    "cookie-parser": "^1.4.0",
    "ejs": "^2.3.4",
    "eventemitter2": "^0.4.14",
    "express": "~4.13.3",
    "express-session": "^1.12.1",
    "glob": "^6.0.1",
    "jquery": "^2.1.0",
    "js-yaml": "~3.4.3",
    "memcached": "^2.2.0",
    "morgan": "~1.6.1",
    "multer": "~1.1.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "redis": "^2.4.2",
    "request": "~2.65.0",
    "superagent": "^1.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.0.12",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.0.12",
    "babel-preset-react": "^6.0.12",
    "babel-runtime": "^6.3.13",
    "cssnano": "^3.4.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.2",
    "grunt": "^0.4.5",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-less": "^1.1.0",
    "grunt-postcss": "^0.7.1",
    "grunt-webpack": "^1.0.11",
    "jest-cli": "^0.8.1",
    "load-grunt-tasks": "^3.3.0",
    "postcss": "^5.0.12",
    "postcss-class-prefix": "^0.3.0",
    "react-addons-test-utils": "^0.14.3",
    "time-grunt": "^1.2.2",
    "webpack": "^1.12.6"
  },
  "engines": {
    "node": ">= 4.1.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest/index.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testDirectoryName": "tests/__tests__",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true
  }
}
